    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>Xan.$(MSBuildProjectName.Replace(" ", "_"))</RootNamespace>
    <AssemblyName>tt</AssemblyName>
